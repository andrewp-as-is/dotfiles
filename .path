#!/bin/sh

export PATH="/usr/libexec:${PATH}"
export PATH="/usr/local/bin:${PATH}"

# macOS
export PATH="/System/Library/Frameworks/CoreServices.framework/Versions/A/Frameworks/LaunchServices.framework/Versions/A/Support:${PATH}"
export PATH="/opt/homebrew/bin:${PATH}"
# brew custom postgres versions
export PATH="/opt/homebrew/opt/postgresql@14/bin:${PATH}"
export PATH="/opt/homebrew/opt/postgresql@15/bin:${PATH}"
export PATH="/opt/homebrew/opt/postgresql@16/bin:${PATH}"
export PATH="/opt/homebrew/opt/postgresql@17/bin:${PATH}"
export PATH="/opt/homebrew/opt/postgresql@18/bin:${PATH}"
export PATH="/opt/homebrew/opt/postgresql@19/bin:${PATH}"
export PATH="/opt/homebrew/opt/postgresql@20/bin:${PATH}"

# go
export GOPATH="${HOME}/.go"

# node
export PATH="${HOME}/.node/bin:${PATH}"

# rust
export PATH="${HOME}/.cargo/bin:${PATH}"

# dotfiles bin
export PATH="${HOME}/.local/share/bin:${PATH}"
export PATH="${HOME}/bin:${PATH}"

